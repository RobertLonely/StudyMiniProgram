<view class="box"><view class="feedback-box"><view class="box-title"><text>意见反馈：</text></view><view class="box-body"><textarea class="text" placeholder="请输入您的意见..." data-event-opts="{{[['input',[['__set_model',['','content','$event',[]]]]]]}}" value="{{content}}" bindinput="__e"></textarea><view class="img"><view class="box-title"><text>反馈图片：</text></view><view class="imgs"><block wx:for="{{imgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="imgs-item"><view data-event-opts="{{[['tap',[['cancelSlect',[index]]]]]}}" class="close" bindtap="__e"><uni-icons vue-id="{{'2d23d9a0-1-'+index}}" type="closeempty" size="20" color="#ffF" bind:__l="__l"></uni-icons></view><view class="img-box"><image src="{{item.url}}" mode></image></view></view></block><block wx:if="{{imgList.length<9}}"><view data-event-opts="{{[['tap',[['selectImg',['$event']]]]]}}" class="imgs-item" bindtap="__e"><view class="add"><uni-icons vue-id="2d23d9a0-2" type="plusempty" size="50" color="#ccc" bind:__l="__l"></uni-icons></view><view class="img-box"></view></view></block></view></view></view><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="submit" bindtap="__e">提交反馈</view></view></view>